{

	"description": "simplifies the process of collecting results of linkedin sales navigator",
	"manifest_version": 2,
	"name": "linkedin sales helper lite",
	"version": "1.20",
	"homepage_url": "http://linkedinsaleshelper.com/",
	"icons": {
		"48": "icons/lsh-48.png"
	},

	"permissions": [
		"downloads",
		"storage",
		"*://*.linkedin.com/sales/*"
	],

	"options_ui": {
		"browser_style": true,
		"open_in_tab": true,
		"page": "settings/options.html"
	},

	"browser_action": {
		"default_icon": "icons/linkedin-sales-helper.png",
		"default_title": "linkedin sales helper lite",
		"default_popup": "popup/toolbox.html"
	},

	"background": {
		"scripts": ["background/csv_downloader.js"]
	}

}
